@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_ReportLayout.cshtml";
}
<h2>Index</h2>
<body>
    <section>
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-12">
                    <div class="card">
                        <div class="card-body">
                            <table id="myTable" class="display">
                                <thead>
                                    <tr>
                                        <th>Column 1</th>
                                        <th>Column 2</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td>Row 1 Data 1</td>
                                        <td>Row 1 Data 2</td>
                                    </tr>
                                    <tr>
                                        <td>Row 2 Data 1</td>
                                        <td>Row 2 Data 2</td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
</body>
<script>
    $(document).ready(function () {
        $('#myTable').DataTable({
            "lengthMenu": [[100, 200, 500, 1000, -1], [100, 200, 500, 1000, "All"]],
            "order": [[0, 'asc'], [1, 'asc']],
            "paging": true,
            dom: 'lBfrtip',
            "bSort": true,
            buttons: [
                'csv', {

                    extend: 'excelHtml5',
                    title: 'Reconstruction_Beneficiary_Detail_Report'
                }, 'pdf'
            ]
        });
    });
</script>

